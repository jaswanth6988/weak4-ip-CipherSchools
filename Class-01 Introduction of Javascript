//data types

//Syntax and declarations

var score = 10;       //ESS-Ecna
let result = 50;      //ES^-Ecmascript 2015
const pi = 3.14       //ES6

score=15;
result = 55;
pi = 3.26

console.log(score);
console.log(result);
console.log(pi);


//Datatypes
/* 1.nimber
   2.Strings
   3.Boolean
   4.Null
   5.Undefined
